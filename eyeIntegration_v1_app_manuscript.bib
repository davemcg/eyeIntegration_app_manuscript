@article{potierMappingGeneRegulatory2014,
  title = {Mapping {{Gene Regulatory Networks}} in {{Drosophila Eye Development}} by {{Large}}-{{Scale Transcriptome Perturbations}} and {{Motif Inference}}},
  volume = {9},
  issn = {2211-1247},
  doi = {10.1016/j.celrep.2014.11.038},
  abstract = {Summary
Genome control is operated by transcription factors (TFs) controlling their target genes by binding to promoters and enhancers. Conceptually, the interactions between TFs, their binding sites, and their functional targets are represented by gene regulatory networks (GRNs). Deciphering in~vivo GRNs underlying organ development in an unbiased genome-wide setting involves identifying both functional TF-gene interactions and physical TF-DNA interactions. To reverse engineer the GRNs of eye development in Drosophila, we performed RNA-seq across 72 genetic perturbations and sorted cell types and inferred a coexpression network. Next, we derived direct TF-DNA interactions using computational motif inference, ultimately connecting 241 TFs to 5,632 direct target genes through 24,926 enhancers. Using this network, we found network motifs, cis-regulatory codes, and regulators of eye development. We validate the predicted target regions of Grainyhead by~ChIP-seq and identify this factor as a general cofactor in the eye network, being bound to thousands of nucleosome-free regions.},
  number = {6},
  journal = {Cell Reports},
  author = {Potier, Delphine and Davie, Kristofer and Hulselmans, Gert and Naval Sanchez, Marina and Haagen, Lotte and {Huynh-Thu}, V\^an Anh and Koldere, Duygu and Celik, Arzu and Geurts, Pierre and Christiaens, Valerie and Aerts, Stein},
  month = dec,
  year = {2014},
  pages = {2290-2303},
  file = {/Users/davidmcgaughey/Zotero/storage/DPPC4X7N/Potier et al. - 2014 - Mapping Gene Regulatory Networks in Drosophila Eye.pdf;/Users/davidmcgaughey/Zotero/storage/UFEUFBMQ/S2211124714010043.html}
}

@article{hayrehSegmentalNatureChoroidal1975,
  title = {Segmental Nature of the Choroidal Vasculature.},
  volume = {59},
  issn = {0007-1161},
  abstract = {The various in vivo studies on posterior ciliary arteries and choroidal vasculature reported piecemeal by the author over the past few years have now been collated. A coherent picture of the vasculature emerges which is of considerable clinical significance. The observations show that the posterior ciliary arteries and their branches right down to the terminal choroidal arterioles, the choriocapillaris, and the vortex veins have a segmental distribution in the choroid, and that the posterior ciliary arteries and choroidal arteries function as end-arteries. No special mascular artery supplying the submacular choroid has been found. The posterior ciliary arteries have only supply the choroid but are also the main source of blood supply to the anterior part of the optic nerve, and have an important role in the blood supply of the retina. The clinical significance of this segmental distribution of the uveal vasculature is discussed.},
  number = {11},
  journal = {The British Journal of Ophthalmology},
  author = {Hayreh, S. S.},
  month = nov,
  year = {1975},
  pages = {631-648},
