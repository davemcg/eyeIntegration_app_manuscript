  author = {Tresini, Maria and Warmerdam, Daniël O. and Kolovos, Petros and Snijder, Loes and Vrouwe, Mischa G. and Demmers, Jeroen A. A. and van IJcken, Wilfred F. J. and Grosveld, Frank G. and Medema, René H. and Hoeijmakers, Jan H. J. and Mullenders, Leon H. F. and Vermeulen, Wim and Marteijn, Jurgen A.},
  options = {useprefix=true},
  file = {/Users/mcgaugheyd/Zotero/storage/ESRSDPFX/Tresini et al. - 2015 - The core spliceosome as target and effector of non.pdf;/Users/mcgaugheyd/Zotero/storage/DVQQDIGP/nature14512.html}
