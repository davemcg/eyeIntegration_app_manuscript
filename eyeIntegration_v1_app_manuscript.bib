  keywords = {Alternative Splicing,Animals,Embryo; Nonmammalian,Gene Expression Regulation,Genetic Complementation Test,HeLa Cells,Humans,Molecular Sequence Data,Mutation,RNA Precursors,Ribonucleoproteins; Small Nuclear,Sequence Analysis; DNA,Zebrafish,Zebrafish Proteins},
  pages = {1965-1976},
  pmid = {21468032}
}

@article{reichReducedNeutrophilCount2009,
  title = {Reduced {{Neutrophil Count}} in {{People}} of {{African Descent Is Due To}} a {{Regulatory Variant}} in the {{Duffy Antigen Receptor}} for {{Chemokines Gene}}},
  volume = {5},
  doi = {10.1371/journal.pgen.1000360},
  abstract = {Author Summary
Many African Americans have white blood cell counts (WBC) that are persistently below the normal range for people of European descent, a condition called ``benign ethnic neutropenia.'' Because most African Americans have both African and European ancestors, selected genetic variants can be analyzed to assign probable African or European origin to each region of each such person's chromosomes. Previously, we found a region on chromosome 1 where increased local African ancestry completely accounted for differences in WBC between African and European Americans, suggesting the presence of an African-derived variant causing low WBC. Here, we show that low neutrophil count is predominantly responsible for low WBC; that a dominant, European-derived allele contributes to high neutrophil count; and that the frequency of this allele differs in Africans and Europeans by $>$91\%. Across the chromosome 1 locus, only the well-characterized ``Duffy'' polymorphism was this differentiated. Neutrophil count was more strongly associated to the Duffy variant than to ancestry, suggesting that the variant itself causes benign ethnic neutropenia. The African, or ``null,'' form of this variant abolishes expression of the ``Duffy Antigen Receptor for Chemokines'' on red blood cells, perhaps altering the concentrations and distribution of chemokines that regulate neutrophil production or migration.},
  number = {1},
  journal = {PLoS Genet},
  author = {Reich, David and Nalls, Michael A. and Kao, W. H. Linda and Akylbekova, Ermeg L. and Tandon, Arti and Patterson, Nick and Mullikin, James and Hsueh, Wen-Chi and Cheng, Ching-Yu and Coresh, Josef and Boerwinkle, Eric and Li, Man and Waliszewska, Alicja and Neubauer, Julie and Li, Rongling and Leak, Tennille S. and Ekunwe, Lynette and Files, Joe C. and Hardy, Cheryl L. and Zmuda, Joseph M. and Taylor, Herman A. and Ziv, Elad and Harris, Tamara B. and Wilson, James G.},
  month = jan,
  year = {2009},
  pages = {e1000360},
  file = {/Users/davidmcgaughey/Zotero/storage/3QUSHFKH/Reich et al. - 2009 - Reduced Neutrophil Count in People of African Desc.pdf;/Users/davidmcgaughey/Zotero/storage/KX3S7V8M/infodoi10.1371journal.pgen.html}
}

@article{raddatzDnmt2dependentMethylomesLack2013,
  title = {Dnmt2-Dependent Methylomes Lack Defined {{DNA}} Methylation Patterns},
  issn = {1091-6490},
  doi = {10.1073/pnas.1306723110},
  abstract = {Several organisms have retained methyltransferase 2 (Dnmt2) as their only candidate DNA methyltransferase gene. However, information about Dnmt2-dependent methylation patterns has been limited to a few isolated loci and the results have been discussed controversially. In addition, recent studies have shown that Dnmt2 functions as a tRNA methyltransferase, which raised the possibility that Dnmt2-only genomes might be unmethylated. We have now used whole-genome bisulfite sequencing to analyze the methylomes of Dnmt2-only organisms at single-base resolution. Our results show that the genomes of Schistosoma mansoni and Drosophila melanogaster lack detectable DNA methylation patterns. Residual unconverted cytosine residues shared many attributes with bisulfite deamination artifacts and were observed at comparable levels in Dnmt2-deficient flies. Furthermore, genetically modified Dnmt2-only mouse embryonic stem cells lost the DNA methylation patterns found in wild-type cells. Our results thus uncover fundamental differences among animal methylomes and suggest that DNA methylation is dispensable for a considerable number of eukaryotic organisms.},
  language = {ENG},
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  author = {Raddatz, G\"unter and Guzzardo, Paloma M and Olova, Nelly and Fantappi\'e, Marcelo Rosado and Rampp, Markus and Schaefer, Matthias and Reik, Wolf and Hannon, Gregory J and Lyko, Frank},
  month = may,
  year = {2013},
  pmid = {23641003}
}

