  date = {2016-06-01},
  pages = {1159-1176},
  author = {Frausto, Ricardo F. and Le, Derek J. and Aldave, Anthony J.},
  file = {/Users/mcgaugheyd/Zotero/storage/VK2QGDUM/Frausto et al. - 2016 - Transcriptomic Analysis of Cultured Corneal Endoth.pdf}
}

