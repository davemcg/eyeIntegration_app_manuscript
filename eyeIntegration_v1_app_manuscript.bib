  author = {Ouyang, Hong and Xue, Yuanchao and Lin, Ying and Zhang, Xiaohui and Xi, Lei and Patel, Sherrina and Cai, Huimin and Luo, Jing and Zhang, Meixia and Zhang, Ming and Yang, Yang and Li, Gen and Li, Hairi and Jiang, Wei and Yeh, Emily and Lin, Jonathan and Pei, Michelle and Zhu, Jin and Cao, Guiqun and Zhang, Liangfang and Yu, Benjamin and Chen, Shaochen and Fu, Xiang-Dong and Liu, Yizhi and Zhang, Kang},
  file = {/Users/mcgaugheyd/Zotero/storage/6BMKYS8D/Ouyang et al. - 2014 - WNT7A and PAX6 define corneal epithelium homeostas.pdf;/Users/mcgaugheyd/Zotero/storage/A7Q6II5Z/nature13465.html}
