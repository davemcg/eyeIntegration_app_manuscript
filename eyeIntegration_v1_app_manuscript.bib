  keywords = {Amino Acid Sequence,Animals,Base Sequence,Binding Sites,Chromosomes; Artificial; Bacterial,Cochlea,Crosses; Genetic,DNA Primers,Deafness,Disease Models; Animal,Gene Library,Hair Cells; Auditory,Membrane Proteins,Mice,Mice; Inbred C3H,Mice; Inbred C57BL,Mice; Inbred Strains,Mice; Mutant Strains,Molecular Sequence Data,Mutagenesis,Pigmentation Disorders,Sequence Alignment,Sequence Homology; Amino Acid,TRPM Cation Channels,Transient Receptor Potential Channels,proteins},
  pages = {14994-14999},
