  author = {Cheng, Linzhao and Hansen, Nancy F. and Zhao, Ling and Du, Yutao and Zou, Chunlin and Donovan, Frank X. and Chou, Bin-Kuan and Zhou, Guangyu and Li, Shijie and Dowey, Sarah N. and Ye, Zhaohui and {NISC Comparative Sequencing Program} and Chandrasekharappa, Settara C. and Yang, Huanming and Mullikin, James C. and Liu, P. Paul},
