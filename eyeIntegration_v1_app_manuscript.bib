  file = {/Users/davidmcgaughey/Zotero/storage/AFH2PJDP/Jiang et al. - 2013 - Sperm, but Not Oocyte, DNA Methylome Is Inherited .pdf;/Users/davidmcgaughey/Zotero/storage/W39V8ATG/S0092-8674(13)00517-5.html}
