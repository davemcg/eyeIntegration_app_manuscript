  year = {2013},
  keywords = {DNA Methylation,Differentiation,Gene expression,Histone modification,Prepatterning,Zebrafish,Zygotic genome activation,development},
  pages = {85-112},
  file = {/Users/davidmcgaughey/Zotero/storage/DBA8RF4P/Andersen et al. - 2013 - Chapter Three - Epigenetic Marking of the Zebrafis.pdf;/Users/davidmcgaughey/Zotero/storage/GN2UHWA8/B9780124160279000036.html}
