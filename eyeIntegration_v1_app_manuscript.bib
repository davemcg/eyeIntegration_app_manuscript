  author = {Whitmore, S. Scott and Wagner, Alex H. and DeLuca, Adam P. and Drack, Arlene V. and Stone, Edwin M. and Tucker, Budd A. and Zeng, Shemin and Braun, Terry A. and Mullins, Robert F. and Scheetz, Todd E.},
  month = dec,
  year = {2014},
  keywords = {Choroid,Gene expression,RNA-Seq,RPE,Retina,Retinitis pigmentosa,Transcriptome,Macular degeneration},
  pages = {93-106},
  file = {/Users/davidmcgaughey/Zotero/storage/MKSUEPQT/Whitmore et al. - 2014 - Transcriptomic analysis across nasal, temporal, an.pdf;/Users/davidmcgaughey/Zotero/storage/34ELQNHU/S0014483514002978.html}
}

