  month = jul,
  year = {2015},
  pages = {53-58},
  file = {/Users/davidmcgaughey/Zotero/storage/ESRSDPFX/Tresini et al. - 2015 - The core spliceosome as target and effector of non.pdf;/Users/davidmcgaughey/Zotero/storage/DVQQDIGP/nature14512.html}
}

