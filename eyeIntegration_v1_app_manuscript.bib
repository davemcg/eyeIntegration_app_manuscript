  author = {Radford, Elizabeth J. and Ito, Mitsuteru and Shi, Hui and Corish, Jennifer A. and Yamazawa, Kazuki and Isganaitis, Elvira and Seisenberger, Stefanie and Hore, Timothy A. and Reik, Wolf and Erkek, Serap and Peters, Antoine H. F. M. and Patti, Mary-Elizabeth and {Ferguson-Smith}, Anne C.},
