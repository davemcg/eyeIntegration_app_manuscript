  author = {Zhang, Yubo and Wong, Chee-Hong and Birnbaum, Ramon Y and Li, Guoliang and Favaro, Rebecca and Ngan, Chew Yee and Lim, Joanne and Tai, Eunice and Poh, Huay Mei and Wong, Eleanor and Mulawadi, Fabianus Hendriyan and Sung, Wing-Kin and Nicolis, Silvia and Ahituv, Nadav and Ruan, Yijun and Wei, Chia-Lin},
  month = dec,
  year = {2013},
  keywords = {Animals,Cell Line,Cell Lineage,Chromatin,Embryonic Stem Cells,Enhancer Elements; Genetic,Gene Expression Regulation,In Situ Hybridization; Fluorescence,Mice,Neural Stem Cells,Promoter Regions; Genetic,RNA Polymerase II,Transcription; Genetic,Zebrafish},
