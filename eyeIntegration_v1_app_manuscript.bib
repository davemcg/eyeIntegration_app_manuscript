  number = {4},
  journal = {Cell},
  author = {Erikson, Galina A. and Bodian, Dale L. and Rueda, Manuel and Molparia, Bhuvan and Scott, Erick R. and {Scott-Van Zeeland}, Ashley A. and Topol, Sarah E. and Wineinger, Nathan E. and Niederhuber, John E. and Topol, Eric J. and Torkamani, Ali},
