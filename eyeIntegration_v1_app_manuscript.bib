}

@article{frankhouserPrEMeRCGInferringNucleotide2014,
  title = {{{PrEMeR}}-{{CG}}: {{Inferring Nucleotide Level DNA Methylation Values}} from {{MethylCap}}-{{Seq Data}}},
  issn = {1367-4811},
  shorttitle = {{{PrEMeR}}-{{CG}}},
  doi = {10.1093/bioinformatics/btu583},
  abstract = {MOTIVATION: DNA methylation is an epigenetic change occurring in genomic CpG sequences that contributes to the regulation of gene transcription both in normal and malignant cells. Next-generation sequencing has been used to characterize DNA methylation status at the genome scale, but suffers from high sequencing cost in the case of whole-genome bisulfite sequencing, or from reduced resolution (inability to precisely define which of the CpGs are methylated) with capture-based techniques.
RESULTS: Here we present a computational method that computes nucleotide-resolution methylation values from capture-based data by incorporating fragment length profiles into a model of methylation analysis. We demonstrate that it compares favorably with nucleotide-resolution bisulfite sequencing and has better predictive power with respect to a reference than window-based methods, often used for enrichment data. The described method was used to produce the methylation data used in tandem with gene expression to produce a novel, clinically significant gene signature in Acute Myeloid Leukemia. In addition, we introduce a complementary statistical method that utilizes this nucleotide-resolution methylation data for detection of differentially methylated features. Availability: Software in the form of Python and R scripts is available at http://bioserv.mps.ohio-state.edu/premer and is free for non-commercial use.
CONTACT: pearlly.yan@osumc.edu; bundschuh@mps.ohio-state.edu SUPPLEMENTARY INFORMATION: Supplementary data are available at Bioinformatics online.},
  language = {ENG},
  journal = {Bioinformatics (Oxford, England)},
  author = {Frankhouser, David E. and Murphy, Mark and Blachly, James S. and Park, Jincheol and Zoller, Mike W. and Ganbat, Javkhlan-Ochir and Curfman, John and Byrd, John C. and Lin, Shili and Marcucci, Guido and Yan, Pearlly and Bundschuh, Ralf},
  month = aug,
  year = {2014},
  pmid = {25178460}
}

@incollection{andersenChapterThreeEpigenetic2013,
  title = {Chapter {{Three}} - {{Epigenetic Marking}} of the {{Zebrafish Developmental Program}}},
  volume = {Volume 104},
  isbn = {0070-2153},
  shorttitle = {Epigenetics and {{Development}}},
  abstract = {Abstract 
A characteristic of anamniote development is a relatively long period of embryonic cell divisions in the absence of on-going transcription. In zebrafish, this period lasts for 10 cell cycles, or $\sim$ 3-h postfertilization, after which zygotic genome activation (ZGA) takes place during the midblastula transition. How the embryo establishes transcriptional competence and how ZGA is spatially and temporally regulated have not been examined until recently. We review here recent data on the transitions in DNA methylation and posttranslational histone modifications occurring during early zebrafish development, as the embryo acquires transcriptional competence and initiates its own gene expression program. We also address models accounting for the origin of epigenetic states detected in early embryos. From these observations, a concept of epigenetic prepatterning of the embryonic gene expression program prior to the onset of ZGA is emerging. The recent data collectively start shedding light on how ZGA may be programmed and regulated.},
