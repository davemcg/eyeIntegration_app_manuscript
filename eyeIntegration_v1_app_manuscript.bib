  author = {Eldred, Kiara C. and Hadyniak, Sarah E. and Hussey, Katarzyna A. and Brenerman, Boris and Zhang, Ping-Wu and Chamling, Xitiz and Sluch, Valentin M. and Welsbie, Derek S. and Hattar, Samer and Taylor, James and Wahlin, Karl and Zack, Donald J. and Johnston, Robert J.},
