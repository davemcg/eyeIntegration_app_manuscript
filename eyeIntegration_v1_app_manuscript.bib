  month = sep,
  year = {2018},
  pages = {425223},
  file = {/Users/davidmcgaughey/Zotero/storage/HRRCYS9J/Lukowski et al. - 2018 - Generation of human neural retina transcriptome at.pdf;/Users/davidmcgaughey/Zotero/storage/AR5F953U/425223.html}
}

@article{luSinglecellRNAseqAnalysis2018,
  title = {Single-Cell {{RNA}}-Seq Analysis Maps the Development of Human Fetal Retina},
  copyright = {\textcopyright{} 2018, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
  doi = {10.1101/423830},
  abstract = {Vision starts with image formation at the retina, which contains diverse neuronal cell types that extract, process, and relay visual information to higher order processing centers in the brain. Though there has been steady progress in defining retinal cell types, very little is known about retinal development in humans, which starts well before birth. In this study, we performed transcriptomic profiling of developing human fetal retina from gestational weeks 12 to 27 using single-cell RNA-seq (scRNA-seq) and used pseudotime analysis to reconstruct the developmental trajectories of retinogenesis. Our analysis reveals transcriptional programs driving differentiation down four different cell types and suggests that M\"uller glia (MG) can serve as embryonic progenitors in early retinal development. In addition, we also show that transcriptional differences separate retinal progenitor cells (RPCs) into distinct subtypes and use this information to reconstruct RPC developmental trajectories and cell fate. Our results support a hierarchical program of differentiation governing cell-type diversity in the developing human retina. In summary, our work details comprehensive molecular classification of retinal cells, reconstructs their relationships, and paves the way for future mechanistic studies on the impact of gene regulation upon human retinogenesis.},
  language = {en},
  journal = {bioRxiv},
  author = {Lu, Yufeng and Yi, Wenyang and Wu, Qian and Zhong, Suijuan and Zuo, Zhentao and Zhao, Fangqi and Zhang, Mei and Tsai, Nicole and Zhuo, Yan and He, Sheng and Zhang, Jun and Duan, Xin and Wang, Xiaoqun and Xue, Tian},
  month = sep,
  year = {2018},
  pages = {423830},
  file = {/Users/davidmcgaughey/Zotero/storage/LJ3VCCM6/Lu et al. - 2018 - Single-cell RNA-seq analysis maps the development .pdf;/Users/davidmcgaughey/Zotero/storage/6CE2DRC8/423830.html}
}

@article{pengMolecularClassificationComparative2018,
  title = {Molecular {{Classification}} and {{Comparative Taxonomics}} of {{Foveal}} and {{Peripheral Cells}} in {{Primate Retina}}},
  copyright = {\textcopyright{} 2018, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NoDerivs 4.0 International), CC BY-ND 4.0, as described at http://creativecommons.org/licenses/by-nd/4.0/},
  doi = {10.1101/428110},
  abstract = {High acuity vision in primates, including humans, is mediated by a small central retinal region called the fovea. As more accessible model organisms lack a fovea, its specialized function and dysfunction in ocular diseases remain poorly understood. We used 165,000 single-cell RNA-seq profiles to generate and validate comprehensive cellular taxonomies of macaque fovea and peripheral retina. More than 80\% of $>$65 cell types match between the two regions, but exhibit substantial differences in proportions and gene expression, some of which we relate to functional differences. Comparison of macaque retinal types with those of mice reveals that interneuron types are tightly conserved, but that projection neuron types and programs diverge, despite conserved transcription factor codes. Key macaque types are conserved in humans, allowing mapping of cell-type and region-specific expression of $>$190 genes associated with 6 human retinal diseases. Our work provides a framework for comparative single-cell analysis across tissue regions and species.},
  language = {en},
  journal = {bioRxiv},
  author = {Peng, Yi-Rong and Shekhar, Karthik and Yan, Wenjun and Herrmann, Dustin and Sappington, Anna and Bryman, Greg S. and van Zyl, Tav\'e and Do, Michael Tri H. and Regev, Aviv and Sanes, Joshua R.},
